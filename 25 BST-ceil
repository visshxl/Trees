question:-https://www.geeksforgeeks.org/problems/implementing-ceil-in-bst/1
int findCeil(Node* root, int x) {
    if (root == NULL) return -1;
    int ceil=root->data;
    while(root){
        if(root->data==x){
            return root->data;
        }else if(root->data > x){
            ceil=root->data;
            root=root->left;
        }else{
            root=root->right;
        }
    }
    return (ceil>=x) ? ceil:-1;
    // Your code here
}
